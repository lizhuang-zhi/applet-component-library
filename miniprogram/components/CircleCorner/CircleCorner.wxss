.circle-all{
  position: fixed;
  bottom: 30%;
  right: 45rpx;
}
.circle-all .item{
  width: 100rpx;
  height: 90rpx;
  z-index: 1000;
}
.circle-all .cir-end{
  z-index: 999;
  position: relative;
}
.circle-all .item image {
  width: 100%;
  height: 100%;
}
.circle-all .cir-start,.cir-item-1{
  opacity: 0;
}
.circle-all .cir-start{
  transform: translateY(180rpx);
}
.circle-all .cir-item-1{
  transform: translateY(86rpx);
}
