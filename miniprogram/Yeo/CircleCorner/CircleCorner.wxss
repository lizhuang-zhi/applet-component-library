.yeo-circle-all{
  position: fixed;
  bottom: 30%;
  right: 45rpx;
}
.yeo-circle-all .yeo-item{
  width: 100rpx;
  height: 90rpx;
  z-index: 1000;
}
.yeo-circle-all .yeo-cir-end{
  z-index: 999;
  position: relative;
}
.yeo-circle-all .yeo-item image {
  width: 100%;
  height: 100%;
}
.yeo-circle-all .yeo-cir-start,.yeo-cir-item-1{
  opacity: 0;
}
.yeo-circle-all .yeo-cir-start{
  transform: translateY(180rpx);
}
.yeo-circle-all .yeo-cir-item-1{
  transform: translateY(86rpx);
}
