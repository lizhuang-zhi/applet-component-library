<view class="yeo-simple-card" style="background: {{BackgroundColor}};box-shadow: 8rpx 8rpx 20rpx {{BackgroundColor}};">
  <view class="card-pic">
    <image src="{{ImgUrl}}"></image>
  </view>
  <view class="card-content">
    <view class="tit">{{title}}</view>
    <view class="desc">{{description}}</view>
  </view>
</view>